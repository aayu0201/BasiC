#include<stdio.h>
#include<conio.h>
#include<math.h>
int main()
{
	int array1[6][6], array2[6][6], final[6][6];
	int a, b, i, j;
	printf("Enter the elements of first matrix\n");
	for(i=0; i<6; i++)
	{
		for(j=0; j<6; j++)
		{
		printf("Element %d%d=", i+1, j+1);
		scanf("%d", &array1[i][j]);
	    }
	}
	printf("Enter the elements of second matrix\n");
	for(i=0; i<6; i++)
	{
		for(j=0; j<6; j++)
		{
		printf("Element %d%d=", i+1, j+1);
		scanf("%d", &array2
		[i][j]);
	    }
	}
	 printf("On Adding Both of the elements we get an other Matrix with\n");
	for(i=0; i<6; i++)
	{
		for(j=0; j<6; j++)
		{
			 final[i][j]= array1[i][j]+array2[i][j];
			
			 
			 printf("Element %d%d =%d\n", i+1, j+1, final[i][j]);
		}
	}
	
}
